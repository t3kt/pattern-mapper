{
  "autogroup": false,
  "groups": [
    {
      "groupname": "blue",
      "paths": "^svg/g\\[id=blue\\]/.*",
      "sequenceby": {"attr":  "distance", "rounddigits":  2}
    },
    {
      "groupname": "teal",
      "paths": "^svg/g\\[id=teal\\]/.*",
      "sequenceby": {"attr":  "distance", "rounddigits":  2}
    },
    {
      "groupname": "red",
      "paths": "^svg/g\\[id=red\\]/.*",
      "sequenceby": {"attr":  "distance", "rounddigits":  2}
    },
    {
      "groupname": "green",
      "paths": "^svg/g\\[id=green\\]/.*",
      "sequenceby": {"attr":  "distance", "rounddigits":  2}
    },
    {
      "suffixes": "rightArm topArm leftArm bottomArm",
      "anglemin": "-20",
      "anglemax": "20",
      "prerotate": "0 -90 -180 -270",
      "sequenceby": {"attr":  "distance", "rounddigits":  2}
    },
    {
      "suffixes": "topRightArm topLeftArm bottomLeftArm bottomRightArm",
      "anglemin": "-20",
      "anglemax": "20",
      "prerotate": "-45 -135 -225 -315"
    },
    {
      "groups": "green",
      "withgroups": "bottomLeftArm bottomRightArm topRightArm topLeftArm",
      "boolop": "and"
    },
    {
      "groups": "blue",
      "withgroups": "bottomArm leftArm topArm rightArm",
      "boolop": "and"
    },
    {
      "groupname": "inner",
      "distancemax": ".3",
      "temporary": true
    },
    {
      "groups": "blue",
      "withgroups": "inner",
      "boolop": "and"
    }
  ],
  "groupshapestates": [
    {
      "group": "blue",
      "_panelcolor": [0, 0.3, 0.8, 1.0],
      "pathcolor": [0, 0.2, 0.9, 1.0],
      "pathtexture": {
        "uvmode": "path",
        "composite": "add",
        "alpha": 0.5,
        "transform": {
          "scale": [0.5, 1.0, 1.0]
        }
      },
      "_texturelayer1": {
        "uvmode": "glob",
        "composite": "over",
        "alpha": 0.7,
        "transform": {
          "scale": [0.9, 1.0, 1.0]
        }
      }
    }
  ]
}